blocks:
- name: center_freq
  id: variable
  parameters:
    comment: 'The frequency that

      the HackRF is tuned to'
    value: 445E6
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [184, 12.0]
    rotation: 0
    state: true
- name: channel1_input
  id: variable_qtgui_range
  parameters:
    comment: 'Select the frequency we

      want to demolate (in MHz)'
    gui_hint: ''
    label: Select Frequency
    min_len: '200'
    orient: QtCore.Qt.Horizontal
    rangeType: float
    start: (center_freq - samp_rate/2) / 1E6
    step: '0.05'
    stop: (center_freq + samp_rate/2) / 1E6
    value: center_freq / 1E6
    widget: dial
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [328, 12.0]
    rotation: 0
    state: true
- name: channel_freq
  id: variable
  parameters:
    comment: 'Convert `channel1_input`

      to Hz'
    value: channel1_input*(1e6)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [496, 12.0]
    rotation: 0
    state: true
- name: channel_width
